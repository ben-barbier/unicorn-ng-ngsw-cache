{
    "index": "/index.html",
    "assetGroups": [
        {
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [
                    "/favicon.ico",
                    "/index.html",
                    "/*.css",
                    "/*.js"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "/assets/**"
                ]
            }
        }
    ],
    "dataGroups": [
        {
            "name": "api-unicorns-list",
            "urls": [
                "/rs/unicorns$"
            ],
            "cacheConfig": {
                "maxSize": 1,
                "maxAge": "1d",
                "timeout": "200u",
                "strategy": "freshness"
            }
        },
        {
            "name": "api-unicorn-detail",
            "urls": [
                "/rs/unicorns/**"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "1d",
                "strategy": "performance"
            }
        }
    ]
}
